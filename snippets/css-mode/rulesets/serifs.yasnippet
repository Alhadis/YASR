# -*- mode: snippet -*-
# key: serifs
# name: Serif font stack
# group: Rulesets
# atom-selector: .source.css
# --
/** Sharp-looking serif-fonts for technical docs */
@font-face {
	font-family: serifs;
	font-weight: normal;
	font-style: normal;
	src:
		local("Cambria"),
		local("Caladea"),
		local("Georgia"),
		local("Linux Libertine"),
		local("Liberation Serif"),
		local("Palatino Linotype"),
		local("Palatino"),
		local("Times New Roman"),
		local("Times"),
		local("serif");
}