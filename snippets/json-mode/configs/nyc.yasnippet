"nyc": {
	"all": true,
	"clean": false,
	"reporter": "text",
	"include": [
		"index.js",
		"register.js"
	],
	"watermarks": {
		"lines":      [80, 95],
		"functions":  [80, 95],
		"branches":   [80, 95],
		"statements": [80, 95]
	}
}