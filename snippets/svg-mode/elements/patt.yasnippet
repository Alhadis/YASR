# -*- mode: snippet -*-
# key: patt
# name: <pattern>
# group: Elements
# atom-selector: .text.xml.svg
# atom-description-more-url: https://mdn.io/SVG/Element/pattern
# --
<pattern id="${1:name}"
	viewBox="0 0 ${1:${2:width}} ${1:${3:height}}"
	width="${1:${2:width}}"
	height="${1:${3:height}}"
	patternTransform="${4:rotate(45)}"
	patternUnits="${5:userSpaceOnUse|objectBoundingBox}"
	preserveAspectRatio="${6:none}"
>$0</pattern>